keywords = {Animals,Background,Bayes Theorem,Conservation of Natural Resources,Demography,Feces,Feces: chemistry,Gene Frequency,Genetic Variation,Genetics, Population,Geography,Hair,Hair: chemistry,Linkage Disequilibrium,Malaysia,Microsatellite Repeats,Microsatellite Repeats: genetics,Pongo pygmaeus,Pongo pygmaeus: genetics,Population Dynamics,Population Genetics},
mendeley-tags = {Background,Population Genetics},
month = {feb},
number = {2},
pages = {441--56},
pmid = {15660936},
title = {{Patterns of genetic diversity and migration in increasingly fragmented and declining orang-utan (Pongo pygmaeus) populations from Sabah, Malaysia.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/15660936},
volume = {14},
year = {2005}
}
