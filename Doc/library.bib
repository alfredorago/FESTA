pages = {3168--3189},
pmid = {19659594},
title = {{Signal design and perception in hypocnemis antbirds: Evidence for convergent evolution via social selection}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19659594 http://doi.wiley.com/10.1111/j.1558-5646.2009.00795.x},
volume = {63},
year = {2009}
