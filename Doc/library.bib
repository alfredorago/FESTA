journal = {Journal Of Statistical Software},
number = {March 2008},
pages = {1--28},
title = {{clValid : An R Package for Cluster Validation}},
url = {ftp://ftp2.it.postgresql.org/pub/mirrors/CRAN/web/packages/clValid/vignettes/clValid.pdf http://citeseerx.ist.psu.edu/viewdoc/download?doi=10.1.1.101.3573{\&}rep=rep1{\&}type=pdf},
