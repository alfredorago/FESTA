issn = {1061-4036},
journal = {Nature genetics},
keywords = {*Meiosis,*Nucleosomes,*Sex Chromosomes,Animals,Chromatin/metabolism,Gene Silencing,Histones/*metabolism,Male,Mice,Pachytene Stage,Protein Structure,Spermatocytes/ultrastructure,Tertiary,Transgenic,Y Chromosome},
month = {feb},
number = {2},
pages = {251--8},
pmid = {17237782},
title = {{Chromosome-wide nucleosome replacement and H3.3 incorporation during mammalian meiotic sex chromosome inactivation.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/17237782},
volume = {39},
year = {2007}
}
