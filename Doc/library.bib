pages = {486--94},
pmid = {17406271},
title = {{A method for parental RNA interference in the wasp Nasonia vitripennis.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/17406271},
volume = {1},
year = {2006}
}
