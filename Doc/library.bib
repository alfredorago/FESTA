pages = {520--537},
pmid = {24520154},
title = {{Machine learning-based differential network analysis: a study of stress-responsive transcriptomes in Arabidopsis}},
url = {http://dx.doi.org/10.1105/tpc.113.121913{\%}5Cnhttp://www.ncbi.nlm.nih.gov/pubmed/24520154{\%}5Cnhttp://www.ncbi.nlm.nih.gov/pmc/articles/PMC3967023{\%}5Cnhttp://www.plantcell.org/cgi/pmidlookup?view=long{\&}pmid=24520154{\%}5Cnhttp://www.plantcell.org/content/26/2/520.},
volume = {26},
