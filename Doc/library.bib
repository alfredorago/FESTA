pages = {1015--1024},
pmid = {19936947},
title = {{Z and W sex chromosomes in the cane toad (Bufo marinus)}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19936947 http://link.springer.com/10.1007/s10577-009-9095-1},
volume = {17},
