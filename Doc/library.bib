month = {apr},
number = {15},
pages = {5460--5},
pmid = {15784743},
title = {{Big brains, enhanced cognition, and response of birds to novel environments.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=556234{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {102},
year = {2005}
}
