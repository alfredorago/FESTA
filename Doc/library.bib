@Article{Sagawa2005a,
  author   = {Sagawa, Kazunori and Yamagata, Hideo and Shiga, Yasuhiro},
  title    = {{Exploring embryonic germ line development in the water flea, Daphnia magna, by zinc-finger-containing VASA as a marker.}},
  journal  = {Gene expression patterns : GEP},
  year     = {2005},
  volume   = {5},
  number   = {5},
  pages    = {669--78},
  month    = {jun},
  issn     = {1567-133X},
  abstract = {VASA is an ATP-dependent RNA helicase belonging to the DEAD-box family that, in many organisms, is specifically expressed in germ line cells throughout the life cycle, making it a powerful molecular marker to study germ line development. To obtain further information on germ line development in crustaceans, we cloned VASA cDNAs from three branchiopod species: water fleas Daphnia magna and Moina macrocopa, and brine shrimp Artemia franciscana. RNA helicase domains in branchiopod VASA were highly conserved among arthropod classes. However, N-terminal RNA-binding domains in branchiopod VASA were highly diverged and, unlike other arthropod VASA reported so far, possessed repeats of retroviral-type zinc finger (CCHC) motifs. Raising specific antibodies against Daphnia VASA revealed that the primordial germ cells (PGCs) in this organism segregate at a very early cleavage stage of embryogenesis in parthenogenetic and sexual eggs. Clusters of PGCs then start to migrate inside the embryo and finally settle at both sides of the intestine, the site of future gonad development. RNA analyses suggested that maternally supplied vasa mRNA was responsible for early VASA expression, while zygotic expression started during blastodermal stage of development.},
  doi      = {10.1016/j.modgep.2005.02.007},
  file     = {:C$\backslash$:/Users/Alfredo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Sagawa, Yamagata, Shiga - 2005 - Exploring embryonic germ line development in the water flea, Daphnia magna, by zinc-finger-containing V.pdf:pdf},
  keywords = {Amino Acid Motifs,Amino Acid Sequence,Animals,Cloning, Molecular,Crustacea,Daphnia,Daphnia: cytology,Daphnia: embryology,Embryo, Nonmammalian,Embryo, Nonmammalian: physiology,Embryonic Development,Female,Germ Cells,Germ Cells: cytology,In Situ Hybridization,Intestines,Intestines: metabolism,Male,Models, Genetic,Molecular Sequence Data,Protein Structure, Tertiary,RNA Helicases,RNA Helicases: biosynthesis,RNA Helicases: chemistry,RNA, Messenger,RNA, Messenger: metabolism,Reverse Transcriptase Polymerase Chain Reaction,Sequence Homology, Amino Acid,Time Factors,Zinc Fingers},
  pmid     = {15939379},
  url      = {http://www.ncbi.nlm.nih.gov/pubmed/15939379},
}

@Article{Shyh-Chang2013a,
  author   = {Shyh-Chang, Ng and Daley, George Q and Cantley, Lewis C},
  title    = {{Stem cell metabolism in tissue development and aging.}},
  journal  = {Development (Cambridge, England)},
  year     = {2013},
  volume   = {140},
  number   = {12},
  pages    = {2535--47},
  month    = {jun},
  issn     = {1477-9129},
  abstract = {Recent advances in metabolomics and computational analysis have deepened our appreciation for the role of specific metabolic pathways in dictating cell fate. Once thought to be a mere consequence of the state of a cell, metabolism is now known to play a pivotal role in dictating whether a cell proliferates, differentiates or remains quiescent. Here, we review recent studies of metabolism in stem cells that have revealed a shift in the balance between glycolysis, mitochondrial oxidative phosphorylation and oxidative stress during the maturation of adult stem cells, and during the reprogramming of somatic cells to pluripotency. These insights promise to inform strategies for the directed differentiation of stem cells and to offer the potential for novel metabolic or pharmacological therapies to enhance regeneration and the treatment of degenerative disease.},
  doi      = {10.1242/dev.091777},
  file     = {:C$\backslash$:/Users/Alfredo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Shyh-Chang, Daley, Cantley - 2013 - Stem cell metabolism in tissue development and aging.pdf:pdf},
  keywords = {Adult Stem Cells,Adult Stem Cells: cytology,Adult Stem Cells: metabolism,Animals,Blastocyst,Blastocyst: cytology,Blastocyst: metabolism,Cell Aging,Cell Differentiation,Cell Proliferation,Embryonic Development,Epigenesis, Genetic,Gene Expression Regulation, Developmental,Glycolysis,Humans,Myoblasts, Skeletal,Myoblasts, Skeletal: cytology,Myoblasts, Skeletal: metabolism,Oxidative Phosphorylation,Oxidative Stress,Pluripotent Stem Cells,Pluripotent Stem Cells: cytology,Pluripotent Stem Cells: metabolism},
  pmid     = {23715547},
  url      = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3666381{\&}tool=pmcentrez{\&}rendertype=abstract},
}

@Article{Bratty2012a,
  author    = {Bratty, Mohammed Al and Chintapalli, Venkateswara R and Dow, Julian a T and Zhang, Tong and Watson, David G},
