@article{Rodriguez2012,
abstract = {Theory predicts that genetic variation in phenotypic plasticity (genotype x environment interaction or G x E) should be eroded by selection acting across environments. However, it appears that G x E is often maintained under selection, although not universally. This variation in the presence and strength of G x E requires explanation. Here I ask whether the explanation may lie in the grain of the environment at which G x E is expressed. The grain (or grain size) of the environment refers to the scale of environmental heterogeneity relative to generation time - that is, relative to the window of operation of selection - with higher rates of heterogeneity occurring in finer-grained environments. The hypothesis that the grain of the environment explains variation in the expression of G x E encapsulates variation in the power of selection to shape reaction norms: selection should be able to erode G x E in fine-grained environments but lose its power as the grain becomes coarser. I survey studies of G x E in sexual traits and demonstrate that the strength of G x E varies with the grain of the environment across which it is expressed, with G x E being stronger in coarser-grained environments. This result elucidates when G x E is most likely to be sustained in the reaction norms of fitness-related traits and when its evolutionary consequences will be most pronounced.},
author = {Rodr{\'{i}}guez, Rafael L.},
doi = {10.1111/j.1420-9101.2012.02565.x},
file = {:C$\backslash$:/Users/Alfredo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Rodr{\'{i}}guez - 2012 - Grain of environment explains variation in the strength of genotype × environment interaction.pdf:pdf},
isbn = {1420-9101 (Electronic)$\backslash$r1010-061X (Linking)},
issn = {1010061X},
journal = {Journal of Evolutionary Biology},
keywords = {Environmental heterogeneity,Genotype-environment interaction,Maintenance of variation},
number = {9},
pages = {1897--1901},
pmid = {22805149},
title = {{Grain of environment explains variation in the strength of genotype × environment interaction}},
volume = {25},
year = {2012}
}
@article{Wang2010,
abstract = {Deep sequencing of transcriptome (RNA-seq) provides unprecedented opportunity to interrogate plausible mRNA splicing patterns by mapping RNA-seq reads to exon junctions (thereafter junction reads). In most previous studies, exon junctions were detected by using the quantitative information of junction reads. The quantitative criterion (e.g. minimum of two junction reads), although is straightforward and widely used, usually results in high false positive and false negative rates, owning to the complexity of transcriptome. Here, we introduced a new metric, namely Minimal Match on Either Side of exon junction (MMES), to measure the quality of each junction read, and subsequently implemented an empirical statistical model to detect exon junctions. When applied to a large dataset ({\textgreater}200M reads) consisting of mouse brain, liver and muscle mRNA sequences, and using independent transcripts databases as positive control, our method was proved to be considerably more accurate than previous ones, especially for detecting junctions originated from low-abundance transcripts. Our results were also confirmed by real time RT-PCR assay. The MMES metric can be used either in this empirical statistical model or in other more sophisticated classifiers, such as logistic regression.},
author = {Wang, Liguo and Xi, Yuanxin and Yu, Jun and Dong, Liping and Yen, Laising and Li, Wei},
doi = {10.1371/journal.pone.0008529},
file = {:C$\backslash$:/Users/Alfredo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Wang et al. - 2010 - A statistical method for the detection of alternative splicing using RNA-seq.pdf:pdf},
issn = {1932-6203},
journal = {PloS one},
keywords = {Alternative Splicing,Exons,Gene Expression Profiling,Logistic Models,Models, Statistical,RNA, Messenger,RNA, Messenger: genetics},
month = {jan},
number = {1},
pages = {e8529},
pmid = {20072613},
title = {{A statistical method for the detection of alternative splicing using RNA-seq.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2798953{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {5},
