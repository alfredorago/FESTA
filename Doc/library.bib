issn = {1432-041X},
journal = {Development genes and evolution},
keywords = {Amino Acid Sequence,Animals,Base Sequence,Cloning, Molecular,Daphnia,Daphnia: genetics,Molecular Sequence Data,RNA Interference,RNA, Double-Stranded,RNA, Double-Stranded: genetics,Sequence Alignment},
month = {mar},
number = {11-12},
pages = {337--45},
pmid = {21327957},
title = {{Development of an RNA interference method in the cladoceran crustacean Daphnia magna.}},
