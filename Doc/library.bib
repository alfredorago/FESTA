doi = {10.1186/gb-2013-14-12-r141},
issn = {1465-6914},
journal = {Genome biology},
month = {jan},
number = {12},
pages = {R141},
pmid = {24359812},
title = {{Obligate mutualism within a host drives the extreme specialization of a fig wasp genome.}},
