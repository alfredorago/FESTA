@article{Bull1982,
abstract = {Living embryos of Mormoniella (Nasonia) vitripennis (Walker) (Hymenoptera: Pteromalidae), were removed from host puparia and studied to describe recognizable stages of embryogenesis that can serve as a basis for experimental studies on this organism. Eight stages and some substages have been defined. Meiosis and nuclear cleavage divisions occur during stages I and II. Pole cell formation is the main event of stage III. Stage IV is characterized by blastoderm formation. Stage V includes cell movements, resulting in gastrulation and germ band extension. The most obvious process seen in stage VI is the invagination of the stomodaeum, and its change in position from a dorsi-anterior position to one that is ventro-anterior. In addition, the serosa forms and segments appear. During stage VII, the completion of organogenesis can be recognized as the digestive tract becomes visible, gonads are formed, and mouthparts are faintly seen. The last stage before hatching is stage VIII. Mouthparts are now distinct, the young larva moves within the chorion, and the tracheae fill with gas. Hatching occurs around 33 hr at 25°C although variability in the length of the embryonic period was observed. These stages are compared to embryonic development in other hymenopterans.},
author = {Bull, Alice Louise},
doi = {10.1016/0020-7322(82)90034-4},
file = {:C$\backslash$:/Users/Alfredo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Bull - 1982 - Stages of living embryos in the jewel wasp Mormoniella (nasonia) vitripennis (walker) (hymenoptera pteromalidae).pdf:pdf},
