doi = {10.1186/1756-8935-5-7},
file = {:C$\backslash$:/Users/Alfredo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Talbert et al. - 2012 - A unified phylogeny-based nomenclature for histone variants.pdf:pdf},
issn = {1756-8935},
