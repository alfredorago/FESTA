@article{Sikkink2016,
abstract = {Phenotypic plasticity has been hypothesized to play a central role in the evolution of phenotypic diversity across species (West-Eberhard ). Through ‘genetic assimilation', phenotypes that are initially environmentally induced within species become genetically fixed over evolutionary time. While genetic assimilation has been shown to occur in both the laboratory and the field (Waddington ; Aubret {\&} Shine ), it remains to be shown whether it can account for broad patterns of phenotypic diversity across entire adaptive radiations. Furthermore, our ignorance of the underlying molecular mechanisms has hampered our ability to incorporate phenotypic plasticity into models of evolutionary processes. In this issue of Molecular Ecology, Parsons et al. () take a significant step in filling these conceptual gaps making use of cichlid fishes as a powerful study system. Cichlid jaw and skull morphology show adaptive, plastic changes in response to early dietary experiences (Fig. 1). In this research, Parsons et al. () first show that the direction of phenotypic plasticity aligns with the major axis of phenotypic divergence across species. They then dissect the underlying genetic architecture of this plasticity, showing that it is specific to the developmental environment and implicating the patched locus in genetic assimilation (i.e. a reduction in the environmental sensitivity of that locus in the derived species). },
author = {Sikkink, Kristin L. and Snell-Rood, Emilie C.},
doi = {10.1111/mec.13926},
file = {:C$\backslash$:/Users/Alfredo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Sikkink, Snell-Rood - 2016 - Plasticity paves the way in an adaptive radiation.pdf:pdf},
