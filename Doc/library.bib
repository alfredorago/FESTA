pages = {365--73},
pmid = {19960122},
title = {{Autofluorescent proteins as photosensitizer in eukaryontes.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2786992{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {6},
