journal = {Nature communications},
keywords = {Animals,Biological Evolution,Branchial Region,Branchial Region: anatomy {\&} histology,Chickens,Embryo, Mammalian,Embryo, Mammalian: anatomy {\&} histology,Embryo, Nonmammalian,Embryo, Nonmammalian: anatomy {\&} histology,Gene Expression Profiling,Gene Expression Profiling: methods,Gene Expression Regulation, Developmental,Mice,Mice, Inbred C57BL,Models, Biological,Organogenesis,Organogenesis: genetics,Phylogeny,RNA, Messenger,RNA, Messenger: analysis,Vertebrates,Vertebrates: embryology,Vertebrates: genetics,Xenopus laevis,Zebrafish},
