month = {may},
number = {1686},
pages = {1435--1442},
pmid = {20071382},
title = {{Positive feedback in the transition from sexual reproduction to parthenogenesis}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2871946{\&}tool=pmcentrez{\&}rendertype=abstract http://rspb.royalsocietypublishing.org/cgi/doi/10.1098/rspb.2009.2113},
