issn = {1553-7404},
journal = {PLoS genetics},
keywords = {Animals,Base Sequence,Drosophila,Drosophila: genetics,Genome, Human,Genotype,Hares,Hares: genetics,High-Throughput Nucleotide Sequencing,Humans,Invertebrates,Invertebrates: genetics,Isoptera,Isoptera: genetics,Metagenomics,Ostreidae,Ostreidae: genetics,Polymorphism, Single Nucleotide,Transcriptome,Transcriptome: genetics,Turtles,Turtles: genetics,Urochordata,Urochordata: genetics,Vertebrates,Vertebrates: genetics},
month = {apr},
