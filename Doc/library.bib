doi = {ARTN e172 DOI 10.1371/journal.pgen.0020172},
issn = {1553-7390},
journal = {Plos Genetics},
keywords = {human gene-expression hypertensive-rats inbred str},
number = {10},
pages = {1625--1633},
title = {{Heritability and tissue specificity of expression quantitative trait loci}},
