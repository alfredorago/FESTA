title = {{Transgenic songbirds offer an opportunity to develop a genetic model for vocal learning.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2764872{\&}tool=pmcentrez{\&}rendertype=abstract},
