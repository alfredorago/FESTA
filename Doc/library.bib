journal = {Evolution},
keywords = {Evo-devo,Evolvability,Modularity,Quantitative genetics},
number = {2},
pages = {301--315},
pmid = {18031304},
title = {{Evolution of evolvability in a developmental model}},
volume = {62},
year = {2008}
