pages = {26},
pmid = {23531303},
title = {{AMBIENT: Active Modules for Bipartite Networks--using high-throughput transcriptomic data to dissect metabolic response.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3656802{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {7},
year = {2013}
}
