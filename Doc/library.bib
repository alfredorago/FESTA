issn = {1758-0463},
journal = {Database},
keywords = {Access to Information,Animal,Animals,Arthropods,Arthropods: genetics,Base Sequence,Databases,Genes,Genetic,Models,Molecular Sequence Annotation,Molecular Sequence Annotation: methods,Species Specificity,Transcriptome,Transcriptome: genetics,User-Computer Interface},
