issn = {1879-1514},
journal = {Aquatic toxicology (Amsterdam, Netherlands)},
keywords = {Animals,Crustacea,Crustacea: drug effects,Crustacea: growth {\&} development,Crustacea: physiology,Disorders of Sex Development,Endocrine Disruptors,Endocrine Disruptors: toxicity,Environment,Hermaphroditic Organisms,Water Pollutants, Chemical,Water Pollutants, Chemical: toxicity},
month = {feb},
