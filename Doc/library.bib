title = {{The impact of opioids on the endocrine system}},
url = {http://journals.lww.com/clinicalpain/Abstract/2009/02000/The{\_}Impact{\_}of{\_}Opioids{\_}on{\_}the{\_}Endocrine{\_}System.15.aspx},
volume = {25},
