file = {:C$\backslash$:/Users/Alfredo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Ma et al. - 2011 - COSINE COndition-SpecIfic sub-NEtwork identification using a global optimization method.pdf:pdf},
issn = {13674803},
journal = {Bioinformatics},
number = {9},
pages = {1290--1298},
pmid = {21414987},
title = {{COSINE: COndition-SpecIfic sub-NEtwork identification using a global optimization method}},
