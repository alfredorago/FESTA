pmid = {20100219},
title = {{Historical divergence and gene flow: Coalescent analyses of mitochondrial, autosomal and sex-linked loci in passerina buntings}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/20100219 http://doi.wiley.com/10.1111/j.1558-5646.2010.00954.x},
volume = {64},
