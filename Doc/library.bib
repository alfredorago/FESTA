month = {feb},
number = {4},
pages = {1671--5},
pmid = {11172009},
title = {{Negative genetic correlation for adult fitness between sexes reveals ontogenetic conflict in Drosophila.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=29315{\&}tool=pmcentrez{\&}rendertype=abstract http://www.pnas.org/cgi/doi/10.1073/pnas.041378098 http://www.ncbi.nlm.nih.gov/pubmed/11172009 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC},
