keywords = {Amino Acid Sequence,Animals,Anthozoa,Anthozoa: genetics,Cloning, Molecular,Fluorescence,Luminescent Proteins,Luminescent Proteins: chemistry,Luminescent Proteins: genetics,Luminescent Proteins: metabolism,Molecular Evolution,Molecular Sequence Data,Phylogeny,Polymerase Chain Reaction,Sequence Alignment,Sequence Homology, Amino Acid,Spectrometry, Fluorescence},
mendeley-tags = {Molecular Evolution},
