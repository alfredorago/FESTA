title = {{The Epigenetic Repertoire of Daphnia magna Includes Modified Histones.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3335717{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {2012},
