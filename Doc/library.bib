month = {jun},
number = {6},
pages = {1543--53},
pmid = {16586445},
title = {{Evidence for the regulation of left-right asymmetry in Ciona intestinalis by ion flux.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/16586445},
volume = {235},
