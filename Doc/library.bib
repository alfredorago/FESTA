@article{Sehnal1985,
abstract = {Morphologyis the oldest branch of entomologicals cience. Intimate knowledge of external structures has provided the background for species identification as well as for the establishment of higher taxa and for considerations on insect phylogeny. Outlines of external and internal morphology of insects can be found in every entomology textbook. The series of monographs by Matsuda (49, 50, 51) is a particularly detailed treatise. In most publications, however, emphasis is given to identification of homologous structures in diverse insects and to their significance for the natural classification of Insecta. Less attention is paid to the developmenotf insect organs during ontogeny, in particular in the course of metamorphosis, which appears externally as a sudden change. Only careful investigations reveal that the seemingly abrupt morphological changes are based on processes extending for considerable lengths of time. The number of such studies has rapidly increased in recent years, because insects are convenient models for research on general problems of developmental biology.  The present review attempts to elucidate the cytological background of morphogenesisin insect postembryonicd evelopment.T he first part illustrates the fundamental principles of tissue morphogenesis. The underlying mechanisms that operate at the subcellular level and the hormonal coordination of tissue development are mentioned only occasionally and very briefly. The second, and principal, part of the review deals with gross morphogenesis of major insect organs. It is shown that imaginal organs differentiate either gradually after hatching or abruptly during metamorphosis. All gradations betweent hese two extremes are found, sometimesa mongt he organs of a single organism. Similarly, development of the same kind of organ differs in various species. Causeso f this diversity in organ developmenatn d in the alterations of overall body shape are briefly treated in the third part of the review.  Insect morphogenesis is the subjecfof a large number of publications of which only a tiny fraction are cited here. Well-established facts are presented without references. Preference in citation is given to papers that describe, either for the first time or with special clearness, interesting developmental phenomena that are in my opinion unknown to many entomologists.},
author = {Sehnal, Frantisek},
doi = {10.1146/annurev.en.30.010185.000513},
file = {:C$\backslash$:/Users/Alfredo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Sehnal - 1985 - Morphology of Insect Development.pdf:pdf},
issn = {0066-4170},
journal = {Annual Review of Entomology},
number = {1},
pages = {89--109},
title = {{Morphology of Insect Development}},
url = {http://www.annualreviews.org/doi/abs/10.1146/annurev.en.30.010185.000513},
volume = {30},
year = {1985}
}
@article{Mallet2005,
abstract = {Hybridization between species is commonplace in plants, but is often seen as unnatural and unusual in animals. Here, I survey studies of natural interspecific hybridization in plants and a variety of animals. At least 25{\%} of plant species and 10{\%} of animal species, mostly the youngest species, are involved in hybridization and potential introgression with other species. Species in nature are often incompletely isolated for millions of years after their formation. Therefore, much evolution of eventual reproductive isolation can occur while nascent species are in gene-flow contact, in sympatry or parapatry, long after divergence begins. Although the relative importance of geographic isolation and gene flow in the origin of species is still unknown, many key processes involved in speciation, such as 'reinforcement' of post-mating isolation by the evolution of assortative mating, will have ample opportunity to occur in the presence of continuing gene flow. Today, DNA sequence data and other molecular methods are beginning to show that limited invasions of the genome are widespread, with potentially important consequences in evolutionary biology, speciation, biodiversity, and conservation.},
author = {Mallet, James},
doi = {10.1016/j.tree.2005.02.010},
issn = {01695347},
journal = {Trends in Ecology and Evolution},
month = {may},
number = {5},
pages = {229--237},
pmid = {16701374},
title = {{Hybridization as an invasion of the genome}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/16701374 http://linkinghub.elsevier.com/retrieve/pii/S016953470500039X},
volume = {20},
year = {2005}
}
