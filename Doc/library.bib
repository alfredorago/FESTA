file = {:C$\backslash$:/Users/Alfredo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/J{\"{o}}ngren et al. - 2010 - Brain gene expression in relation to fearfulness in female red junglefowl (Gallus gallus).pdf:pdf},
issn = {1601-183X},
journal = {Genes, brain, and behavior},
keywords = {chicken,domestication,fear,gene expres-},
month = {oct},
number = {7},
pages = {751--8},
pmid = {20597989},
title = {{Brain gene expression in relation to fearfulness in female red junglefowl (Gallus gallus).}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/20597989},
volume = {9},
year = {2010}
}
