keywords = {G-matrix,M-matrix,genetic integration,mutation},
number = {12},
pages = {3213--3225},
pmid = {27678053},
title = {{Epistasis and pleiotropy affect the modularity of the genotype-phenotype map of cross-resistance in HIV-1.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/27678053{\%}5Cnhttp://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5100054},
