doi = {10.1016/j.tpb.2007.08.006},
file = {:C$\backslash$:/Users/Alfredo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/King, Masel - 2007 - The evolution of bet-hedging adaptations to rare scenarios.pdf:pdf},
