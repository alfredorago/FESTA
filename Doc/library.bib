journal = {Comparative biochemistry and physiology. Part D, Genomics {\&} proteomics},
keywords = {Amino Acid Sequence,Animals,Daphnia,Daphnia: chemistry,Daphnia: genetics,Daphnia: metabolism,Dopamine,Dopamine: genetics,Dopamine: metabolism,Genomics,Molecular Sequence Data,Octopamine,Octopamine: genetics,Octopamine: metabolism,Proteome,Proteome: chemistry,Proteome: genetics,Proteome: metabolism,Sequence Alignment,Serotonin,Serotonin: genetics,Serotonin: metabolism,Signal Transduction},
