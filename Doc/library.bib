journal = {American Naturalist},
keywords = {Metapopulation,Migration,costs,ecotypes,environmental conditions,environmental heterogeneity,evolution,model,phenotypic plasticity,specialization,speciation},
number = {2},
pages = {271--283},
pmid = {1854},
title = {{Metapopulation structure faviors plasticity over local adaptation}},
volume = {160},
year = {2002}
