journal = {BioEssays : news and reviews in molecular, cellular and developmental biology},
keywords = {Adaptation, Biological,Adaptation, Biological: genetics,Animals,Biological Evolution,Environment,Gene Expression,Genetic Variation,Genetics, Population,Growth and Development,Growth and Development: genetics,Models, Genetic,Phenotype,Selection, Genetic},
month = {jan},
