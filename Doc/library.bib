keywords = {Evolution of mutational covariance,Evolutionary constraint,Evolvability,Genetic covariance matrix,Phenotypic plasticity},
number = {9},
pages = {2891--2902},
pmid = {22946810},
title = {{Phenotypic plasticity facilitates mutational variance, genetic variance, and evolvability along the major axis of environmental variation}},
