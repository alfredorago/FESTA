issn = {1355-8382},
journal = {RNA},
keywords = {3' Untranslated Regions,Base Sequence,Messenger,Messenger: genetics,Molecular Sequence Data,Open Reading Frames,RNA,Untranslated,Untranslated: genetics},
month = {apr},
number = {4},
pages = {825--843},
pmid = {22361292},
title = {{Computational analysis of functional long noncoding RNAs reveals lack of peptide-coding capacity and parallels with 3' UTRs}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3312569{\&}tool=pmcentrez{\&}rendertype=abstract http://rnajournal.cshlp.org/cgi/doi/10.1261/rna.029520.111},
