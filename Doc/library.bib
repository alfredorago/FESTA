title = {{Genome-wide association between DNA methylation and alternative splicing in an invertebrate.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3526459{\&}tool=pmcentrez{\&}rendertype=abstract},
