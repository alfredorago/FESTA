month = {jul},
number = {27},
pages = {11166--71},
pmid = {19564601},
title = {{Invasive hybrid tiger salamander genotypes impact native amphibians.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2703667{\&}tool=pmcentrez{\&}rendertype=abstract},
