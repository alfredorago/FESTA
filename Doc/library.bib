issn = {1559-2308},
journal = {Epigenetics : official journal of the DNA Methylation Society},
keywords = {atherosclerosis,burden in chronic kidney,cardiovascular disease,ckd,disease,epigenetic dysregulation contributes to,epigenetics,hemodialysis,kidney disease,mace,mirna,next-generation sequencing,the high cardiovascular disease},
month = {jan},
