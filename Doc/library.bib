pmid = {8928226},
title = {{How is myogenesis initiated in the embryo?}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/8928226 http://www.sciencedirect.com/science/article/B6TCY-3W25BNB-5T/2/39cfd916ceb9a1f6d3be11fee60361cf},
volume = {12},
