journal = {The Annals of Statistics},
month = {aug},
number = {4},
pages = {1165--1188},
pmid = {18298808},
title = {{The control of the false discovery rate in multiple testing under depencency}},
url = {http://projecteuclid.org/Dienst/getRecord?id=euclid.aos/1013699998/ http://arxiv.org/abs/0801.1095 http://dx.doi.org/10.1214/08-AOS620 http://www.ncbi.nlm.nih.gov/pubmed/18298808 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC2375137},
volume = {29},
