issn = {00129658},
journal = {Ecology},
keywords = {advantages,asex-,by combining sexual and,cladocera,crowding,cyclic parthenogens,cyclical parthenogenesis,daphnia magna,diapause,environ-,life cycle,mental sex determination,photoperiod,reproductive strategy,sex ratio adjustment,should benefit from the,ual reproduction},
number = {6},
pages = {2255--2268},
title = {{Sex determination in Daphnia magna}},
url = {http://www.jstor.org.libraryproxy.griffith.edu.au/stable/1938637 http://www.jstor.org/stable/10.2307/1938637},
