journal = {The American Naturalist},
keywords = {5289 university of oregon,adaptation,ary biology,center for ecology and,corresponding author,e-mail,eugene,evolution-,fish,g max,genetic covariance,genetic drift,melanotaenia,oregon 97403-5289,present address,rainbow},
month = {jan},
number = {1},
