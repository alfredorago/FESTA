journal = {Nucleic acids research},
keywords = {and phrases,as well as with,gene copy-,homology searches,number profiles and sequence,of available data,of newly sequenced,orthodb also provides mapping,with the explosive growth},
month = {jan},
number = {Database issue},
pages = {D358--65},
pmid = {23180791},
title = {{OrthoDB: a hierarchical catalog of animal, fungal and bacterial orthologs.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3531149{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {41},
