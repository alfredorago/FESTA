doi = {10.1098/rstb.2011.0287},
file = {:C$\backslash$:/Users/Alfredo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Baker et al. - 2012 - Gene duplication, tissue-specific gene expression and sexual conflict in stalk-eyed flies (Diopsidae).pdf:pdf},
