pages = {4133--4138},
pmid = {24591634},
title = {{BMP signaling is required for the generation of primordial germ cells in an insect}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24591634 http://www.pnas.org/cgi/doi/10.1073/pnas.1400525111},
volume = {111},
year = {2014}
