keywords = {Animals,Body Patterning,Body Patterning: genetics,Cell Lineage,Drosophila melanogaster,Female,Gastrulation,Gene Expression Profiling,Gene Expression Regulation, Developmental,Mesoderm,Mesoderm: metabolism,Phylogeny,Signal Transduction,Species Specificity,Tribolium,Wasps,Wasps: genetics,Wasps: physiology},
month = {sep},
