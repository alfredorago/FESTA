pages = {71--7},
pmid = {9987908},
title = {{Phylogenetic relationships, evolution, and genetic diversity of the domestic dog.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/9987908},
volume = {90},
year = {1999}
}
