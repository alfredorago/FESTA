pages = {1638--1643},
pmid = {14757817},
title = {{Major events in the genome evolution of vertebrates: Paranome age and size differ considerably between ray-finned fishes and land vertebrates}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=341801{\&}tool=pmcentrez{\&}rendertype=abstract http://www.pnas.org/cgi/doi/10.1073/pnas.0307968100},
