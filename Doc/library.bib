pages = {3622--7},
pmid = {20139301},
title = {{Complete HOX cluster characterization of the coelacanth provides further evidence for slow evolution of its genome.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2840454{\&}tool=pmcentrez{\&}rendertype=abstract},
