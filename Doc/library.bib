journal = {Genetics},
keywords = {Analysis of Variance,Animals,Chromosome Mapping,Crosses, Genetic,Female,Genes, Insect,Genes, Insect: genetics,Inheritance Patterns,Inheritance Patterns: genetics,Male,Ploidies,Selection, Genetic,Sex Determination Processes,Temperature,Wasps,Wasps: genetics,Wasps: growth {\&} development,Wasps: physiology},
month = {mar},
