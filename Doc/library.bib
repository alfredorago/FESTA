keywords = {Gene Regulatory Networks,Gene Regulatory Networks: genetics,Humans,MicroRNAs,MicroRNAs: biosynthesis,MicroRNAs: genetics,MicroRNAs: metabolism,Neoplasms,Neoplasms: genetics,Oncogenes,Oncogenes: genetics},
month = {aug},
number = {8},
pages = {1348--54},
pmid = {20227518},
title = {{mir-17-92, a cluster of miRNAs in the midst of the cancer network.}},
