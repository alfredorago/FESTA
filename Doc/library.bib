journal = {Toxicological sciences : an official journal of the Society of Toxicology},
keywords = {Acute,Animals,Cadmium,Cadmium: toxicity,Daphnia,Daphnia: physiology,Dinitrophenols,Dinitrophenols: toxicity,Ecotoxicology,Fourier Transform Infrared,Hemolymph,Hemolymph: chemistry,Hemolymph: drug effects,Hemolymph: metabolism,Metabolomics,Methods,Nitriles,Nitriles: toxicity,Propranolol,Propranolol: toxicity,Pyrethrins,Pyrethrins: toxicity,Spectroscopy,Toxicity Tests,Toxicogenetics,Toxicogenetics: methods,Xenobiotics,Xenobiotics: metabolism,Xenobiotics: toxicity},
