pmid = {14966531},
title = {{Functional bias and spatial organization of genes in mutational hot and cold regions in the human genome.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=340940{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {2},
