mendeley-tags = {3' Untranslated Regions,Base Sequence,Brain,Down-Regulation,Genes,Humans,Messenger,MicroRNAs,Molecular Sequence Data,Muscles,Mutation,Oligonucleotide Array Sequence Analysis,Organ Specificity,RNA,Reporter,Substrate Specificity},
month = {feb},
