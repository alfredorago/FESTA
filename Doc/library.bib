isbn = {1558-5646 (Electronic)$\backslash$n0014-3820 (Linking)},
issn = {00143820},
journal = {Evolution},
keywords = {Adaptive evolution,DNA methylation,Epigenetic marking,Genetic accommodation,Genetic assimilation,Phenotypic plasticity},
number = {3},
pages = {656--672},
