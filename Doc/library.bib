journal = {Chemosphere},
keywords = {Animals,Animals, Newborn,Daphnia,Daphnia: drug effects,Daphnia: physiology,Fatty Acids, Unsaturated,Fatty Acids, Unsaturated: toxicity,Female,Insecticides,Insecticides: toxicity,Juvenile Hormones,Juvenile Hormones: toxicity,Male,Phenylcarbamates,Phenylcarbamates: toxicity,Sex Ratio,Terpenes,Terpenes: toxicity,Water Pollutants, Chemical,Water Pollutants, Chemical: toxicity},
month = {dec},
