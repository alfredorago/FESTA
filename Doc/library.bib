doi = {10.1073/pnas.1405550111},
file = {:C$\backslash$:/Users/Alfredo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Wilson, Nowak - 2014 - Natural selection drives the evolution of ant life cycles.pdf:pdf},
