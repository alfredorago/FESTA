journal = {IEEE Transactions on Evolutionary Computation},
keywords = {Automatic problem decomposition,evolutionary computation,linkage learning,modularity,scalability},
number = {5},
pages = {628--642},
title = {{Transforming evolutionary search into higher-level evolutionary search by capturing problem structure}},
