keywords = {Amino Acid Sequence,Animals,DNA,DNA: metabolism,Drosophila,Drosophila Proteins,Epigenetics,Gene Silencing,Genes,Genetic,Histones,Histones: metabolism,Insect,Insect Proteins,Insect Proteins: metabolism,Molecular Sequence Data,Promoter Regions,Protein Binding,Repressor Proteins,Repressor Proteins: metabolism,Reverse Transcriptase Polymerase Chain Reaction,Transcription Factors,Transcription Factors: metabolism},
mendeley-tags = {Epigenetics,Histones},
month = {aug},
number = {August},
pages = {3--7},
pmid = {11493924},
title = {{General transcription factors bind promoters repressed by Polycomb group proteins}},
