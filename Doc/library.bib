@article{Cruickshank2008,
abstract = {A central goal of evolutionary developmental biology (Evo-Devo) is to synthesize comparative molecular developmental genetics and its description of the dynamic relationship between genotype and phenotype with the microevolutionary processes (mutation, random drift, and selection) of population genetics. To this end, we analyzed sequence variation of five gene classes that act sequentially to shape early embryo development in Drosophila: maternal, gap, pair-rule, segment polarity, and segment identity genes. We found two related patterns: (1) a microevolutionary pattern, wherein relative sequence variation within species is 2- to 3-fold higher for maternal-effect genes than for any other gene class; and, (2) a macroevolutionary pattern, wherein the relative sequence divergence among species for maternal-effect genes is 2- to 4-fold greater than for any other gene class. Both patterns are qualitatively and quantitatively consistent with the predictions of microevolutionary theory. Our findings connect within-species genetic variation to between-species divergence and shed light on the controversy over the existence of a "developmental hourglass," where mid-embryonic stages are more evolutionarily constrained than either earlier or later stages. Because maternal-effect genes experience relaxed selective constraint relative to zygotic-effect genes, they explore a wider mutational and phenotypic space. As a result, early acting maternal-effect genes diverge more widely across taxa and thereby broaden the base of the developmental hourglass. In contrast, later acting zygotic genes are relatively more constrained and limited in their diversification across taxa, narrowing the waist of the developmental hourglass. This pattern is obscured by genes with both maternal and zygotic expression, which experience the strongest evolutionary constraint.},
author = {Cruickshank, Tami and Wade, Michael J},
doi = {10.1111/j.1525-142X.2008.00273.x},
file = {:C$\backslash$:/Users/Alfredo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Cruickshank, Wade - 2008 - Microevolutionary support for a developmental hourglass gene expression patterns shape sequence variation and.pdf:pdf},
issn = {1520541X},
journal = {Evolution {\&} Development},
keywords = {Animals,Biological Evolution,Developmental,Drosophila,Drosophila: genetics,Gene Expression Regulation,Genetic Variation,Genomic Imprinting},
month = {sep},
number = {5},
pages = {583--590},
pmid = {18803776},
title = {{Microevolutionary support for a developmental hourglass: gene expression patterns shape sequence variation and divergence in Drosophila}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/18803776 http://doi.wiley.com/10.1111/j.1525-142X.2008.00273.x},
volume = {10},
year = {2008}
}
