journal = {The American Naturalist},
keywords = {Animals,Biological Evolution,Culture,DNA,DNA: genetics,Environment,Epigenesis,Female,Genetic,Heredity,Humans,Male,Maternal Behavior,Models,RNA,RNA: genetics,Selection},
month = {aug},
number = {2},
