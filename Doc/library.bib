pages = {e11191},
pmid = {20585648},
title = {{Evolutionary dead end in the Gal{\'{a}}pagos: divergence of sexual signals in the rarest of Darwin's finches.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2890412{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {5},
